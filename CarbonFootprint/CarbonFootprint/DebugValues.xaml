<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CarbonFootprint.DebugValues">
    <ContentPage.Content>
        <StackLayout>
                <Label x:Name="NameField" HorizontalTextAlignment="Center" VerticalTextAlignment="Start"/>
                <Label x:Name="CarName" HorizontalTextAlignment="Center" VerticalTextAlignment="Start"/>
                <Label x:Name="CarAge" HorizontalTextAlignment="Center" VerticalTextAlignment="Start"/>
                <Label x:Name="CarBuildYear" HorizontalTextAlignment="Center" VerticalTextAlignment="Start"/>
                <Label x:Name="GasMilage" HorizontalTextAlignment="Center" VerticalTextAlignment="Start"/>
                <Label x:Name="CarType" HorizontalTextAlignment="Center" VerticalTextAlignment="Start"/>
                <Label x:Name="HouseEnergy" HorizontalTextAlignment="Center" VerticalTextAlignment="Start"/>
                <Label x:Name="CubicVolume" HorizontalTextAlignment="Center" VerticalTextAlignment="Start"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>